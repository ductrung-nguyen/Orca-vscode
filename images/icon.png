<svg width="128" height="128" viewBox="0 0 128 128" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="128" height="128" rx="16" fill="#1e3a8a"/>
  
  <!-- Atom symbol -->
  <g transform="translate(64, 64)">
    <!-- Nucleus -->
    <circle cx="0" cy="0" r="8" fill="#fbbf24"/>
    
    <!-- Electron orbits -->
    <ellipse cx="0" cy="0" rx="35" ry="15" fill="none" stroke="#60a5fa" stroke-width="2.5" opacity="0.8"/>
    <ellipse cx="0" cy="0" rx="35" ry="15" fill="none" stroke="#60a5fa" stroke-width="2.5" opacity="0.8" transform="rotate(60)"/>
    <ellipse cx="0" cy="0" rx="35" ry="15" fill="none" stroke="#60a5fa" stroke-width="2.5" opacity="0.8" transform="rotate(120)"/>
    
    <!-- Electrons -->
    <circle cx="35" cy="0" r="4" fill="#f87171"/>
    <circle cx="-17.5" cy="30.3" r="4" fill="#34d399"/>
    <circle cx="-17.5" cy="-30.3" r="4" fill="#a78bfa"/>
  </g>
  
  <!-- Text "ORCA" -->
  <text x="64" y="110" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#ffffff" text-anchor="middle">ORCA</text>
</svg>
